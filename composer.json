{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "name": "bits/oneapp",
            "type": "vcs",
            "url": "https://gitlab.bitsamericas.net/cristian.delacruz/oneapp.git"
        },
        {
            "name": "bits/oneapp_convergent",
            "type": "vcs",
            "url": "https://gitlab.bitsamericas.net/oneapp/oneapp_convergent.git"
        },
        {
            "name": "bits/oneapp_mobile",
            "type": "vcs",
            "url": "https://gitlab.bitsamericas.net/oneapp/oneapp_mobile.git"
        },
        {
            "name": "bits/oneapp_home",
            "type": "vcs",
            "url": "https://gitlab.bitsamericas.net/oneapp/oneapp_home.git"
        },
        {
            "name": "bits/aws_service",
            "type": "vcs",
            "url": "https://gitlab.bitsamericas.net/oneapp/aws_service.git"
        },
        {
            "name": "bits/adf_simple_auth",
            "type": "vcs",
            "url": "https://gitlab.bitsamericas.net/oneapp/adf_simple_auth.git"
        },
        {
            "name": "bits/adf_devsecops",
            "type": "vcs",
            "url": "https://gitlab.bitsamericas.net/oneapp/adf_devsecops.git"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "bits/adf_devsecops": "1.2.3",
        "bits/adf_simple_auth": "3.0.2",
        "bits/aws_service": "3.0.1",
        "bits/oneapp": "3.0.2",
        "bits/oneapp_convergent": "3.0.9",
        "bits/oneapp_home": "3.0.8",
        "bits/oneapp_mobile": "3.0.10",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/admin_audit_trail": "^1.0@alpha",
        "drupal/admin_toolbar": "^3.0",
        "drupal/backup_migrate": "^5.0",
        "drupal/config_split": "^2.0@RC",
        "drupal/console": "^1.0.2",
        "drupal/core": "9.2.7",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-project-message": "^9",
        "drupal/core-recommended": "^9",
        "drupal/devel_entity_updates": "^3.0",
        "drupal/devel_php": "^1.3",
        "drupal/key": "^1.14",
        "drupal/mailsystem": "^4.3",
        "drupal/memcache": "^2.0",
        "drupal/mobile_detect": "^3.0",
        "drupal/monolog": "^2.0@beta",
        "drupal/panels": "^4.6",
        "drupal/queue_ui": "^2.2",
        "drupal/redis": "^1.5",
        "drupal/registration_role": "^1.1",
        "drupal/restui": "^1.20",
        "drupal/role_delegation": "^1.1",
        "drupal/simplesamlphp_auth": "3.2",
        "drupal/smtp": "^1.0",
        "drupal/token": "^1.9",
        "drush/drush": "^9.7.1 | ^10.0.0",
        "predis/predis": "^1.1",
        "vlucas/phpdotenv": "^4.0",
        "webflo/drupal-finder": "^1.0.0"
    },
    "require-dev": {
        "drupal/core-dev": "^9",
        "zaporylie/composer-drupal-optimizations": "^1.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "discard-changes": true,
        "sort-packages": true,
        "process-timeout": 600,
        "allow-plugins": {
            "composer/installers": true,
            "simplesamlphp/composer-module-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/console-extend-plugin": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "zaporylie/composer-drupal-optimizations": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/redis": {
                "OMLATAM-29421 - Default lifetime for TTL": "https://www.drupal.org/files/issues/2020-09-23/redis-default_permanent_ttl-2944938.patch"
            }
        }
    }
}
